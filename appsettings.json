{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "logging": "Information",
        "logging.Data": "Debug"
      },
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "..\\logs\\webapi-.log",
            "rollingInterval": "Day"
          }
        }
      ]
    },
    "AllowedHosts": "*"
  }
}